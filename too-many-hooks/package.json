{
  "name": "too-many-hooks",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/too-many-hooks.umd.cjs",
  "module": "./dist/too-many-hooks.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/too-many-hooks.js",
      "require": "./dist/too-many-hooks.umd.cjs"
    }
  },
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint:code": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0",
    "lint:formatting": "prettier --check src",
    "lint:types": "tsc --noEmit",
    "lint": "yarn lint:code && yarn lint:formatting && yarn lint:types"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.29.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "typescript": "4.8.4",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1"
  },
  "packageManager": "yarn@3.3.0"
}
